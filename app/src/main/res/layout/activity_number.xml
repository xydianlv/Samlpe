<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:orientation="horizontal"
    android:baselineAligned="false"
    android:background="@color/color_white"
    android:paddingTop="24.0dp"
    android:paddingBottom="24.0dp"
    >

  <FrameLayout
      android:layout_width="0.0dp"
      android:layout_weight="1"
      android:layout_height="match_parent"
      >

    <TextView
        android:id="@+id/game_num_score_a"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/number"
        android:textColor="@color/color_262626"
        android:layout_gravity="center_horizontal"
        />

    <View
        android:layout_width="2.0dp"
        android:layout_height="320.0dp"
        android:layout_gravity="center"
        android:background="@color/color_b7d28d"
        />

    <ImageView
        android:id="@+id/game_num_avatar_a"
        android:layout_width="46.0dp"
        android:layout_height="46.0dp"
        android:contentDescription="@string/image"
        android:src="@mipmap/multi_image_6"
        android:scaleType="fitCenter"
        android:layout_gravity="center_horizontal|bottom"
        />

  </FrameLayout>

  <LinearLayout
      android:layout_width="0.0dp"
      android:layout_weight="3"
      android:layout_height="match_parent"
      android:orientation="vertical"
      android:gravity="center_horizontal"
      >

    <TextView
        android:id="@+id/game_num_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/number"
        android:textColor="@color/color_262626"
        android:textSize="24.0dp"
        />

    <View
        android:layout_width="0.0dp"
        android:layout_height="24.0dp"
        />

    <TextView
        android:id="@+id/game_num_target"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/number"
        android:textColor="@color/color_262626"
        android:textSize="18.0dp"
        />

    <View
        android:layout_width="0.0dp"
        android:layout_height="160.0dp"
        />

    <TextView
        android:id="@+id/game_num_info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/number"
        android:textColor="@color/color_84919b"
        android:textSize="18.0dp"
        />

    <View
        android:layout_width="0.0dp"
        android:layout_height="36.0dp"
        />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        >

      <TextView
          android:id="@+id/game_num_fun_add"
          android:layout_width="32.0dp"
          android:layout_height="32.0dp"
          android:gravity="center"
          android:text="@string/add"
          android:textSize="18.0dp"
          android:background="@color/color_f2f4f6"
          />

      <View
          android:layout_width="6.0dp"
          android:layout_height="0.0dp"
        />

      <TextView
          android:id="@+id/game_num_fun_sub"
          android:layout_width="32.0dp"
          android:layout_height="32.0dp"
          android:gravity="center"
          android:text="@string/subject"
          android:textSize="18.0dp"
          android:background="@color/color_f2f4f6"
          />

      <View
          android:layout_width="6.0dp"
          android:layout_height="0.0dp"
          />

      <TextView
          android:id="@+id/game_num_fun_mul"
          android:layout_width="32.0dp"
          android:layout_height="32.0dp"
          android:gravity="center"
          android:text="@string/multiply"
          android:textSize="18.0dp"
          android:background="@color/color_f2f4f6"
          />

      <View
          android:layout_width="6.0dp"
          android:layout_height="0.0dp"
          />

      <TextView
          android:id="@+id/game_num_fun_delete"
          android:layout_width="32.0dp"
          android:layout_height="32.0dp"
          android:gravity="center"
          android:text="@string/delete"
          android:textSize="18.0dp"
          android:background="@color/color_f2f4f6"
          />

      <View
          android:layout_width="6.0dp"
          android:layout_height="0.0dp"
          />

      <TextView
          android:id="@+id/game_num_fun_left"
          android:layout_width="32.0dp"
          android:layout_height="32.0dp"
          android:gravity="center"
          android:text="@string/k_left"
          android:textSize="18.0dp"
          android:background="@color/color_f2f4f6"
          />

      <View
          android:layout_width="6.0dp"
          android:layout_height="0.0dp"
          />

      <TextView
          android:id="@+id/game_num_fun_right"
          android:layout_width="32.0dp"
          android:layout_height="32.0dp"
          android:gravity="center"
          android:text="@string/k_right"
          android:textSize="18.0dp"
          android:background="@color/color_f2f4f6"
          />

    </LinearLayout>

    <View
        android:layout_width="0.0dp"
        android:layout_height="28.0dp"
        />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >

      <TextView
          android:id="@+id/game_num_data_a"
          android:layout_width="32.0dp"
          android:layout_height="32.0dp"
          android:gravity="center"
          android:text="@string/number"
          android:textSize="18.0dp"
          android:background="@color/color_f2f4f6"
          />

      <View
          android:layout_width="6.0dp"
          android:layout_height="0.0dp"
          />

      <TextView
          android:id="@+id/game_num_data_b"
          android:layout_width="32.0dp"
          android:layout_height="32.0dp"
          android:gravity="center"
          android:text="@string/number"
          android:textSize="18.0dp"
          android:background="@color/color_f2f4f6"
          />

      <View
          android:layout_width="6.0dp"
          android:layout_height="0.0dp"
          />

      <TextView
          android:id="@+id/game_num_data_c"
          android:layout_width="32.0dp"
          android:layout_height="32.0dp"
          android:gravity="center"
          android:text="@string/number"
          android:textSize="18.0dp"
          android:background="@color/color_f2f4f6"
          />

      <View
          android:layout_width="6.0dp"
          android:layout_height="0.0dp"
          />

      <TextView
          android:id="@+id/game_num_data_d"
          android:layout_width="32.0dp"
          android:layout_height="32.0dp"
          android:gravity="center"
          android:text="@string/number"
          android:textSize="18.0dp"
          android:background="@color/color_f2f4f6"
          />

      <View
          android:layout_width="6.0dp"
          android:layout_height="0.0dp"
          />

      <TextView
          android:id="@+id/game_num_data_delete"
          android:layout_width="70.0dp"
          android:layout_height="32.0dp"
          android:gravity="center"
          android:text="@string/k_delete"
          android:textSize="18.0dp"
          android:background="@color/color_f2f4f6"
          />

    </LinearLayout>

  </LinearLayout>

  <FrameLayout
      android:layout_width="0.0dp"
      android:layout_weight="1"
      android:layout_height="match_parent"
      >

    <TextView
        android:id="@+id/game_num_score_b"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/number"
        android:textColor="@color/color_262626"
        android:layout_gravity="center_horizontal"
        />

    <View
        android:layout_width="2.0dp"
        android:layout_height="320.0dp"
        android:layout_gravity="center"
        android:background="@color/color_b7d28d"
        />

    <ImageView
        android:id="@+id/game_num_avatar_b"
        android:layout_width="46.0dp"
        android:layout_height="46.0dp"
        android:contentDescription="@string/image"
        android:src="@mipmap/multi_image_9"
        android:scaleType="fitCenter"
        android:layout_gravity="center_horizontal|bottom"
        />

  </FrameLayout>

</LinearLayout>