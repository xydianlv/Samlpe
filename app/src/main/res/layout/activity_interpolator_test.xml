<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:background="@color/color_white"
    android:fitsSystemWindows="true"
    >

  <include
      layout="@layout/common_toolbar"
      />

  <FrameLayout
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      >

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:overScrollMode="never"
        android:scrollbars="none"
        android:background="@null"
        >

      <LinearLayout
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:orientation="vertical"
          >

        <com.example.wyyu.gitsamlpe.test.anim.interpolator.InterpolatorTestView
            android:id="@+id/interpolator_test_0"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:animType="0"
            />

        <com.example.wyyu.gitsamlpe.test.anim.interpolator.InterpolatorTestView
            android:id="@+id/interpolator_test_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:animType="1"
            />

        <com.example.wyyu.gitsamlpe.test.anim.interpolator.InterpolatorTestView
            android:id="@+id/interpolator_test_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:animType="2"
            />

        <com.example.wyyu.gitsamlpe.test.anim.interpolator.InterpolatorTestView
            android:id="@+id/interpolator_test_3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:animType="3"
            />

        <com.example.wyyu.gitsamlpe.test.anim.interpolator.InterpolatorTestView
            android:id="@+id/interpolator_test_4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:animType="4"
            />

        <com.example.wyyu.gitsamlpe.test.anim.interpolator.InterpolatorTestView
            android:id="@+id/interpolator_test_5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:animType="5"
            />

        <com.example.wyyu.gitsamlpe.test.anim.interpolator.InterpolatorTestView
            android:id="@+id/interpolator_test_6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:animType="6"
            />


        <com.example.wyyu.gitsamlpe.test.anim.interpolator.InterpolatorTestView
            android:id="@+id/interpolator_test_7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:animType="7"
            />

        <com.example.wyyu.gitsamlpe.test.anim.interpolator.InterpolatorTestView
            android:id="@+id/interpolator_test_8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:animType="8"
            />

        <com.example.wyyu.gitsamlpe.test.anim.interpolator.InterpolatorTestView
            android:id="@+id/interpolator_test_9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:animType="9"
            />

        <com.example.wyyu.gitsamlpe.test.anim.interpolator.InterpolatorTestView
            android:id="@+id/interpolator_test_10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:animType="10"
            />

        <com.example.wyyu.gitsamlpe.test.anim.interpolator.InterpolatorTestView
            android:id="@+id/interpolator_test_11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:animType="11"
            />

        <com.example.wyyu.gitsamlpe.test.anim.interpolator.InterpolatorTestView
            android:id="@+id/interpolator_test_12"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:animType="12"
            />

      </LinearLayout>

    </ScrollView>

    <TextView
        android:id="@+id/interpolator_test_start"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:layout_gravity="bottom"
        android:gravity="center"
        android:textSize="@dimen/text_16"
        android:textColor="@color/color_b7d28d"
        android:padding="@dimen/padding_14"
        android:text="@string/start_all"
        android:background="@drawable/bg_round_grey_green_10"
        />

  </FrameLayout>

</LinearLayout>