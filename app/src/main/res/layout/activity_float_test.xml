<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/color_white"
    android:gravity="center_horizontal"
    >

  <include
      layout="@layout/common_toolbar"
      />

  <TextView
      android:id="@+id/float_button"
      android:text="@string/float_test"
      android:layout_marginTop="60.0dp"
      style="@style/ClickableText"
      />

  <TextView
      android:id="@+id/float_dialog"
      android:text="@string/dialog_test"
      android:layout_marginTop="48.0dp"
      style="@style/ClickableText"
      />

  <TextView
      android:id="@+id/float_sys"
      android:text="@string/float_sys"
      style="@style/ClickableText"
      />

  <TextView
      android:id="@+id/live_service"
      android:text="@string/live_service"
      style="@style/ClickableText"
      />

</LinearLayout>